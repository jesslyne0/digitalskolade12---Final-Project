create or replace view JESSLYN.PUBLIC.JESSLYN_TOTALORDER_PERCOUNTRY_PERMONTH(
	ORDER_MONTH,
	SHIP_COUNTRY,
	TOTAL_ORDER
) as
SELECT * FROM (
SELECT SUBSTR(A.ORDER_DATE,1,7) ORDER_MONTH, A.SHIP_COUNTRY, COUNT(A.ORDER_ID) TOTAL_ORDER
FROM ORDERS A
GROUP BY SUBSTR(A.ORDER_DATE,1,7), A.SHIP_COUNTRY
);